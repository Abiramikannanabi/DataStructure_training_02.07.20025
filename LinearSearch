package linearseacrh;
import java.util.*;
public class Linear {

	public static void main(String[] args) {
		int a[]= {1,5,2,9,3};
		Scanner sc=new Scanner(System.in);
		int key=sc.nextInt();
		int count=0;
		for(int i=0;i<a.length;i++) 
		{
			if(a[i]==key)
			{
				System.out.println("Element found at "+i);
				count=1;

			}
			
		}
		if(count==0)
		{
			System.out.println("Element not found");
		}
	}

}
